{
  "name": "vcbswf",
  "version": "1.0.0",
  "description": "personal workflow setting",
  "main": "index.js",
  "scripts": {
    "scss": "gulp sass",
    "php-s": "php -S 127.0.0.1:1234 -t ./mock/",
