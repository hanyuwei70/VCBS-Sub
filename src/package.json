  },
  "keywords": [
    "vcbs"
  ],
  "author": "inori",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "bower": "^1.7.9",
    "browser-sync": "^2.12.8",
    "eslint": "^2.10.2",
    "fms": "0.0.31",
    "gulp": "^3.9.1",
    "gulp-eslint": "^2.0.0",
    "gulp-line-ending-corrector": "^1.0.1",
    "gulp-plumber": "^1.0.1",
    "gulp-postcss": "^6.1.1",
    "gulp-sass": "^2.3.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-stylus": "^2.3.1",
    "gulp-uglify": "^1.5.1",
    "nodemon": "^1.9.2",
    "npm-run-all": "^1.8.0",
    "postcss-scss": "^0.1.8",
    "run-sequence": "^1.2.0",
    "stylelint": "^6.5.1",
    "stylelint-config-standard": "^7.0.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
